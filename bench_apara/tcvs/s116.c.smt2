(declare-rel loop ((Array Int Int) Int Int ))
(declare-rel exit ())
(declare-var i Int )
(declare-var i1 Int )
(declare-var index_limit Int )
(declare-var a_i Int )
(declare-var a_i_plus_1 Int )
(declare-var a_i_plus_2 Int )
(declare-var a_i_plus_3 Int )
(declare-var a_i_plus_4 Int )
(declare-var a_i_plus_5 Int )
(declare-var a_i_new Int )
(declare-var a_i_plus_1_new Int )
(declare-var a_i_plus_2_new Int )
(declare-var a_i_plus_3_new Int )
(declare-var a_i_plus_4_new Int )
(declare-var a_i_plus_5_new Int )
(declare-var count Int )
(declare-var a_array (Array Int Int) )
(declare-var a_array1 (Array Int Int) )
(declare-var a_array2 (Array Int Int) )
(declare-var a_array3 (Array Int Int) )
(declare-var a_array4 (Array Int Int) )
(declare-var a_array5 (Array Int Int) )

(rule (=> 
	(and 
		(= index_limit (- (* count 8) 5))
		(= i 0)
	)
	(loop a_array i index_limit)
))
(rule (=> 
	(and 
		(loop a_array i index_limit)
		(< i index_limit)
		(= a_i (select a_array i))
		(= a_i_plus_1 (select a_array (+ i 1)))
		(= a_i_plus_2 (select a_array (+ i 2)))
		(= a_i_plus_3 (select a_array (+ i 3)))
		(= a_i_plus_4 (select a_array (+ i 4)))
		(= a_i_plus_5 (select a_array (+ i 5)))
		(= a_i_new (* a_i_plus_1 a_i))
		(= a_i_plus_1_new (* a_i_plus_2 a_i_plus_1))
		(= a_i_plus_2_new (* a_i_plus_3 a_i_plus_2))
		(= a_i_plus_3_new (* a_i_plus_4 a_i_plus_3))
		(= a_i_plus_4_new (* a_i_plus_5 a_i_plus_4))
		(= a_array1 (store a_array i a_i_new))
		(= a_array2 (store a_array1 (+ i 1) a_i_plus_1_new))
		(= a_array3 (store a_array2 (+ i 2) a_i_plus_2_new))
		(= a_array4 (store a_array3 (+ i 3) a_i_plus_3_new))
		(= a_array5 (store a_array4 (+ i 4) a_i_plus_4_new))
		(= i1 (+ i 5))
	)
	(loop a_array5 i1 index_limit)
))
(rule (=> 
	(and 
		(loop a_array i index_limit)
		(not (< i index_limit))
	)
	exit
))
(query exit)